            columnData[counter++] = $($($.parseHTML(inputData)).val(data.Nombre_del_Representado)).addClass('Nombre_del_Representado').attr('id', nameOfTable + 'Nombre_del_Representado' + rowIndex).attr('data-field', 'Nombre_del_Representado');
