                $('<td id="'+nameOfTable+'NUC'+rowIndex+'">').text(data.NUC).appendTo(row);
            else 
                $('<td style="display:none;" id="'+nameOfTable+'NUC'+rowIndex+'">').text(data.NUC).appendTo(row);
