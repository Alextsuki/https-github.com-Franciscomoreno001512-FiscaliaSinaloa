        function Detalle_de_AbogadorowCreationGrid(row, data, index) {
            var permission =  {            
                 Edit: '@permissionDetalle_de_Abogado.Edit' == 'True',
                 Delete: '@permissionDetalle_de_Abogado.Delete' == 'True'
            };
            setDetalle_de_AbogadoValidation();
            var Detalle_de_AbogadoRowElement = "Detalle_de_Abogado_" + index.toString();
            $(row).addClass(Detalle_de_AbogadoRowElement);
            var nameOfTable='Detalle_de_Abogado_';
            var rowIndex = '_' + index;
            Detalle_de_AbogadoinsertRowCurrentIndex = index;

            row.innerHTML = "";

            if (data.IsInsertRow) {

                if (row.childNodes.length != 0) {
                    for (var i = 0; i < row.childNodes.length; i++) {
                        row.childNodes[i].remove();
                    }
                }
                var controls = GetInsertDetalle_de_AbogadoRowControls(index);
                var actionColInsert = $('<td>');

                var abc = "if(dynamicFieldValidation('" + Detalle_de_AbogadoRowElement + "')){ Detalle_de_AbogadoInsertRow(" + index + "); }";
                var insertRowHTML = '<a  data-toggle="tooltip" title="Confirmar Registro" onclick="' + abc + '">';
                $('<i class="fa fa-check">').appendTo($(insertRowHTML).appendTo(actionColInsert));
				$('<i class="fa fa-times">').appendTo($("<a data-toggle='tooltip' title='Cancelar Registro' onclick='Detalle_de_AbogadoCancelRow(" + index + ")'>").appendTo(actionColInsert));
                actionColInsert.appendTo(row);

                for (i = 0; i < controls.length; i++) {
					var idHeader = $(controls[i]).data('field') + 'Header';
					if($(controls[i]).length > 1){
						idHeader = $($(controls[i])[1]).data('field') + 'Header';
					}					

					var classe = ($('#Detalle_de_AbogadoGrid .' + idHeader).hasClass('dt-right') ? "dt-right" : "") + $('#Detalle_de_AbogadoGrid .' + idHeader ).css('display') == 'none' ? " hide" : "";
				   
					if ($(controls[i]).next().length > 0) {
					    var div = $(controls[i]).next();
					    $('<td class="' + classe + '">').append($(controls[i])).append(div).appendTo(row);
					}else
					    $(controls[i]).appendTo($('<td class="' + classe + '">').appendTo(row));
					
                }
                $('#Detalle_de_AbogadoGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                $('#Detalle_de_AbogadoGrid .gridDatePicker').attr("readonly", "");
                return;
            }

            var actionCol = $('<td>');
			var edit = "<a title='@Resources.Resources.Edit' class='edit-MR' onclick='Detalle_de_AbogadoEditRow(" + index + ", this)'>";
