                url: "@Url.Content("~/Frontal/Audiencias_MP/Get_Datos_Generales")?Id=" + $("#ReferenceClave").val(),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                                        $('#Clave').val(result.data.Clave);
                    var control = $('#Expediente_MP');
                    control.select2('open');
                    $('.select2-search__field').val(result.data.Expediente_MP).trigger('keyup');
                    control.select2('close');
                    setTimeout(function () {
	                    var data = eval('AutoComplete' + control.selector.replace('#', '') + 'Data');
	                    control.select2({ data: data });
	                    control.val(result.data.Expediente_MP).trigger('change');
                    }, 3000);
                    $('#Tipo_de_Audiencia').val(result.data.Tipo_de_Audiencia);
                    $('#Audiencia').val(result.data.Audiencia);
                    $('#Fecha').val(result.data.Fecha);
                    $('#Hora').val(result.data.Hora);
                    Detalle_Audiencia_Defensor_ImputadoDataDataTable = result.Defensor_del_Imputado;
                    showDetalle_Audiencia_Defensor_ImputadoGrid(result.Defensor_del_Imputado);
                    $('#Detalle_Audiencia_Defensor_ImputadoGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_Audiencia_Defensor_ImputadoGrid .gridDatePicker').attr("readonly", "");
                    Detalle_Audiencia_Defensor_VictimaDataDataTable = result.Defensor_por_Victima;
                    showDetalle_Audiencia_Defensor_VictimaGrid(result.Defensor_por_Victima);
                    $('#Detalle_Audiencia_Defensor_VictimaGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_Audiencia_Defensor_VictimaGrid .gridDatePicker').attr("readonly", "");
                    $('#Observaciones').val(result.data.Observaciones);
                    $('#Resultado_de_Audiencia').val(result.data.Resultado_de_Audiencia);
                    Detalle_Audiencia_Archivos_AdjuntosDataDataTable = result.Archivos;
                    showDetalle_Audiencia_Archivos_AdjuntosGrid(result.Archivos);
                    $('#Detalle_Audiencia_Archivos_AdjuntosGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_Audiencia_Archivos_AdjuntosGrid .gridDatePicker').attr("readonly", "");
                    $('#Asignar_MP_Litigacion').prop('checked', result.data.Asignar_MP_Litigacion);
                    var control = $('#MP_Litigacion');
                    control.select2('open');
                    $('.select2-search__field').val(result.data.MP_Litigacion).trigger('keyup');
                    control.select2('close');
                    setTimeout(function () {
	                    var data = eval('AutoComplete' + control.selector.replace('#', '') + 'Data');
	                    control.select2({ data: data });
	                    control.val(result.data.MP_Litigacion).trigger('change');
                    }, 3000);

                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
		
		function Save_Datos_Generales(newId) {
			var data = new FormData();
			data.append('Clave', newId);
			                data.append('Expediente_MP', $('#Expediente_MP').val());
                data.append('Tipo_de_Audiencia', $('#Tipo_de_Audiencia').val());
                data.append('Audiencia', $('#Audiencia').val());
                data.append('Fecha', $('#Fecha').val());
                data.append('Hora', $('#Hora').val());
                data.append('Observaciones', $('#Observaciones').val());
                data.append('Resultado_de_Audiencia', $('#Resultado_de_Audiencia').val());
                data.append('Asignar_MP_Litigacion', $('#Asignar_MP_Litigacion').prop('checked'));
                data.append('MP_Litigacion', $('#MP_Litigacion').val());

			$.ajax({
				url: '@Url.Content("~/Frontal/Audiencias_MP/Post_Datos_Generales")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
				contentType: false,
				processData: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "Error al guardar Datos_Generales");
					} else {
						newId = result;
                    					    SendDetalle_Audiencia_Defensor_Imputado(null, newId);
					    SendDetalle_Audiencia_Defensor_Victima(null, newId);
					    SendDetalle_Audiencia_Archivos_Adjuntos(null, newId);

					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "Error al guardar Datos_Generales");
				},
				cache: false
			});
		}


    </script>
