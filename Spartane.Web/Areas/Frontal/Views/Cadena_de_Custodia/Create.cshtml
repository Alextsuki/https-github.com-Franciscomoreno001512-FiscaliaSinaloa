        function Catalogo_Tipo_de_Embalaje_CCrowCreationGrid(row, data, index) {
            var permission =  {            
                 Edit: '@permissionCatalogo_Tipo_de_Embalaje_CC.Edit' == 'True',
                 Delete: '@permissionCatalogo_Tipo_de_Embalaje_CC.Delete' == 'True'
            };
            setCatalogo_Tipo_de_Embalaje_CCValidation();
            var Catalogo_Tipo_de_Embalaje_CCRowElement = "Catalogo_Tipo_de_Embalaje_CC_" + index.toString();
            $(row).addClass(Catalogo_Tipo_de_Embalaje_CCRowElement);
            var nameOfTable='Catalogo_Tipo_de_Embalaje_CC_';
            var rowIndex = '_' + index;
            Catalogo_Tipo_de_Embalaje_CCinsertRowCurrentIndex = index;

            row.innerHTML = "";

            if (data.IsInsertRow) {

                if (row.childNodes.length != 0) {
                    for (var i = 0; i < row.childNodes.length; i++) {
                        row.childNodes[i].remove();
                    }
                }
                var controls = GetInsertCatalogo_Tipo_de_Embalaje_CCRowControls(index);
                var actionColInsert = $('<td>');

                var abc = "if(dynamicFieldValidation('" + Catalogo_Tipo_de_Embalaje_CCRowElement + "')){ Catalogo_Tipo_de_Embalaje_CCInsertRow(" + index + "); }";
                var insertRowHTML = '<a  data-toggle="tooltip" title="Confirmar Registro" onclick="' + abc + '">';
                $('<i class="fa fa-check">').appendTo($(insertRowHTML).appendTo(actionColInsert));
				$('<i class="fa fa-times">').appendTo($("<a data-toggle='tooltip' title='Cancelar Registro' onclick='Catalogo_Tipo_de_Embalaje_CCCancelRow(" + index + ")'>").appendTo(actionColInsert));
                actionColInsert.appendTo(row);

                for (i = 0; i < controls.length; i++) {
					var idHeader = $(controls[i]).data('field') + 'Header';
					if($(controls[i]).length > 1){
						idHeader = $($(controls[i])[1]).data('field') + 'Header';
					}					

					var classe = ($('#Catalogo_Tipo_de_Embalaje_CCGrid .' + idHeader).hasClass('dt-right') ? "dt-right" : "") + $('#Catalogo_Tipo_de_Embalaje_CCGrid .' + idHeader ).css('display') == 'none' ? " hide" : "";
				   
					if ($(controls[i]).next().length > 0) {
					    var div = $(controls[i]).next();
					    $('<td class="' + classe + '">').append($(controls[i])).append(div).appendTo(row);
					}else
					    $(controls[i]).appendTo($('<td class="' + classe + '">').appendTo(row));
					
                }
                $('#Catalogo_Tipo_de_Embalaje_CCGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                $('#Catalogo_Tipo_de_Embalaje_CCGrid .gridDatePicker').attr("readonly", "");
                return;
            }

            var actionCol = $('<td>');
			var edit = "<a title='@Resources.Resources.Edit' class='edit-MR' onclick='Catalogo_Tipo_de_Embalaje_CCEditRow(" + index + ", this)'>";
            if (!permission.Edit || isConsultMode)
                edit = "";
            var editPopUp = "<a title='@Resources.Resources.Edit' class='edit-popup-MR' data-toggle='modal' data-target='#AddCatalogo_Tipo_de_Embalaje_CC-form' onclick='Catalogo_Tipo_de_Embalaje_CCEditRowPopup(" + index + ", this)'>";
            if (!permission.Edit || isConsultMode)
                editPopUp = "";
            var eliminar = "<a title='@Resources.Resources.Delete' class='eliminar-MR' onclick='Catalogo_Tipo_de_Embalaje_CCDeleteRow(" + index + ", this)'>";
            if (!permission.Delete || isConsultMode)
                eliminar = "";
            $('<i class="fa fa-pencil">').appendTo($(edit).appendTo(actionCol));
            //$('<i class="fa fa-edit">').appendTo($(editPopUp).appendTo(actionCol));
            $('<i class="fa fa-trash-o">').appendTo($(eliminar).appendTo(actionCol));
			if ($('.Action' + 'Header').css('display') != 'none')
				actionCol.appendTo(row);

            if ($('.Descripcion' + 'Header').css('display') != 'none')
                $('<td id="'+nameOfTable+'Descripcion'+rowIndex+'">').text(data.Descripcion).appendTo(row);
