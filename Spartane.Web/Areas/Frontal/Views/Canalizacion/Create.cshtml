                    $('#Nueva_Relacion').val(result.data.Nueva_Relacion);
                    $('#ResolucionId').val(result.data.ResolucionId);
                    $('#Resolucion').val(result.data.Resolucion);
                    $('#Detalle_de_la_Resolucion').val(result.data.Detalle_de_la_Resolucion);
                    $('#Observaciones').val(result.data.Observaciones);
                    Detalle_Diligencias_CanalizacionDataDataTable = result.Diligencias_Generadas;
                    showDetalle_Diligencias_CanalizacionGrid(result.Diligencias_Generadas);
                    $('#Detalle_Diligencias_CanalizacionGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_Diligencias_CanalizacionGrid .gridDatePicker').attr("readonly", "");

                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
		
		function Save_Resolucion(newId) {
			var data = new FormData();
			data.append('Clave', newId);
			                data.append('Fecha_de_Resolucion', $('#Fecha_de_Resolucion').val());
                data.append('Hora_de_Resolucion', $('#Hora_de_Resolucion').val());
                data.append('Hubo_modificacion_en_la_relacion', $('#Hubo_modificacion_en_la_relacion').prop('checked'));
                data.append('Nueva_Relacion', $('#Nueva_Relacion').val());
                data.append('ResolucionId', $('#ResolucionId').val());
                data.append('Resolucion', $('#Resolucion').val());
                data.append('Detalle_de_la_Resolucion', $('#Detalle_de_la_Resolucion').val());
                data.append('Observaciones', $('#Observaciones').val());

			$.ajax({
				url: '@Url.Content("~/Frontal/Canalizacion/Post_Resolucion")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
				contentType: false,
				processData: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "Error al guardar Resolucion");
					} else {
						newId = result;
                    					    SendDetalle_Diligencias_Canalizacion(null, newId);

					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "Error al guardar Resolucion");
				},
				cache: false
			});
		}


    </script>
