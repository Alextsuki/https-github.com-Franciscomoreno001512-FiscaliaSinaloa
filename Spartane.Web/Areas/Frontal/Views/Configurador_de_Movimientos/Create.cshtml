		function Fill_Datos_Generales()
        {
            $.ajax({
                url: "@Url.Content("~/Frontal/Configurador_de_Movimientos/Get_Datos_Generales")?Id=" + $("#ReferenceClave").val(),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                                        $('#Clave').val(result.data.Clave);
                    $('#Descripcion').val(result.data.Descripcion);
                    $('#Fase').val(result.data.Fase);
                    $('#Aplica_para_Adolescentes').prop('checked', result.data.Aplica_para_Adolescentes);
                    Detalle_Datos_Adicionales_MovimientoDataDataTable = result.Datos_Adicionales;
                    showDetalle_Datos_Adicionales_MovimientoGrid(result.Datos_Adicionales);
                    $('#Detalle_Datos_Adicionales_MovimientoGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_Datos_Adicionales_MovimientoGrid .gridDatePicker').attr("readonly", "");

                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
		
		function Save_Datos_Generales(newId) {
			var data = new FormData();
			data.append('Clave', newId);
			                data.append('Descripcion', $('#Descripcion').val());
                data.append('Fase', $('#Fase').val());
                data.append('Aplica_para_Adolescentes', $('#Aplica_para_Adolescentes').prop('checked'));

			$.ajax({
				url: '@Url.Content("~/Frontal/Configurador_de_Movimientos/Post_Datos_Generales")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
				contentType: false,
				processData: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "Error al guardar Datos_Generales");
					} else {
						newId = result;
                    					    SendDetalle_Datos_Adicionales_Movimiento(null, newId);

					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "Error al guardar Datos_Generales");
				},
				cache: false
			});
		}


