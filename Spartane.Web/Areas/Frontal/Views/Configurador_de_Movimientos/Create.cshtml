        function getDetalle_Datos_Adicionales_MovimientoData() {
            $.ajax({
                url: "@Url.Content("~/Frontal/Configurador_de_Movimientos/GetDetalle_Datos_Adicionales_Movimiento")" + "?draw=1&start=1&length=2147483647" + ($("#ReferenceClave").val() != "" ? "&RelationId=" + $("#ReferenceClave").val() : ""),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                    Detalle_Datos_Adicionales_MovimientoDataDataTable = result.data;
                    showDetalle_Datos_Adicionales_MovimientoGrid(result.data);
                    $('#Detalle_Datos_Adicionales_MovimientoGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_Datos_Adicionales_MovimientoGrid .gridDatePicker').attr("readonly", "");
                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
        function Detalle_Datos_Adicionales_MovimientoGetUpdateRowControls(data, nameOfTable, rowIndex) {

            var columnData = [];
            var counter = 0;
            var inputData = "<input type='text' class='fullWidth form-control' />";

