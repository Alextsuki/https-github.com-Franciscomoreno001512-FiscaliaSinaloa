			$('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldId').val(fieldId);
			$('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldName').val(fieldName);
			$('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleAttribute').val(attribute);
		});
