        function initiateUIControls() {
            $('.gridDatePicker').datepicker({
                todayBtn: "linked",
                keyboardNavigation: false,
                forceParse: false,
                calendarWeeks: true,
                autoclose: true,
                format: "@ConfigurationProperty.DateFormatDatePicker"
            });
            //Time Picker
            $('.gridTimePicker').clockpicker();

        }
		
		function SendDetalle_Aseguramiento_Medios_de_TransporteData(callBack) {
			
					
		 var arr = []; 
		 $("form#CreateDetalle_Aseguramiento_Medios_de_Transporte").find(':input').each(function( index ) {		 
			if ($(this).prop('disabled')==true|| $(this).prop('disabled')=="disabled")
			{		
				arr.push($(this).attr('id')); 
				$(this).prop('disabled', false);  	
			}
	    });
		
             var formData = new FormData($("form#CreateDetalle_Aseguramiento_Medios_de_Transporte")[0]);
			$.each( arr, function( key, value ) {
			  $('#' +value).prop('disabled', true);
			});

            var isNew = $('#hdnIsNew').val();
           
            if (checkClientValidate('Detalle_Aseguramiento_Medios_de_TransporteCreateForm')) {
                $.ajax({
                    url: '@Url.Content("~/Frontal/Detalle_Aseguramiento_Medios_de_Transporte/Post?IsNew=")' + isNew,
                    type: 'POST',
                    data: formData,
                    async: true,
                    success: function (result) {
                        if (result < 0) {
                            showNotification("@Resources.Resources.ErrorSave", "error");
                        } else {

                            showNotification("@Resources.Resources.SaveSuccess", "success");
                            callBack(result);
                        }
                    },
                    error: function (result) {
                        showNotification("@Resources.Resources.ErrorContactingServer", "error");
                    },
                    cache: false,
                    contentType: false,
                    processData: false
                });
            }
            return false;
        };
		
        function GetPropertyBag() {
            $.get('@Url.Action("Detalle_Aseguramiento_Medios_de_TransportePropertyBag", "Detalle_Aseguramiento_Medios_de_Transporte")', function (data) {
                $(".PropertyModal").html(data);
            });
        }
        function Detalle_Aseguramiento_Medios_de_TransporteGetBusinessRule(FieldId, FieldName, Attribute) {
			if (FieldId != null && FieldName != null)
			{
				$('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldId').val(FieldId);
				$('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldName').val(FieldName);
				$('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleAttribute').val(Attribute);
			}
            $.get('@Url.Action("BusinessRule", "BussinessRule")', { FieldId: $('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldId').val(), FieldName: $('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldName').val(), ObjectId: '45528', Attribute: $('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleAttribute').val(), Screen: 'Detalle_Aseguramiento_Medios_de_Transporte' }, function (data) {
                $(".Detalle_Aseguramiento_Medios_de_TransporteBusinessRuleModal").html(data);
            });
        }
        function Detalle_Aseguramiento_Medios_de_TransporteGetAddBusinessRule(RuleId, FieldId, FieldName, Attribute) {
			if (FieldId != null && FieldName != null) {
				$('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldId').val(FieldId);
				$('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldName').val(FieldName);
				$('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleAttribute').val(Attribute);
			}
            $.get('@Url.Action("AddBusinessRule", "BussinessRule")', { BusinessRuleId: RuleId, FieldId: $('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldId').val(), FieldName: $('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldName').val(), ObjectId: '45528', Attribute: $('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleAttribute').val(), Screen: 'Detalle_Aseguramiento_Medios_de_Transporte' }, function (data) {
                $(".Detalle_Aseguramiento_Medios_de_TransporteAddBusinessRuleModal").html(data);
                $(".datatable").DataTable();
            });
        }
		function RemoveBusinessRule(RuleId) {
			$.get('@Url.Action("RemoveBusinessRule", "BussinessRule")', { BusinessRuleId: RuleId, FieldId: $('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldId').val(), FieldName: $('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleFieldName').val(), ObjectId: '45528', Attribute: $('#hdnDetalle_Aseguramiento_Medios_de_TransporteBusinessRuleAttribute').val(), Screen: 'Detalle_Aseguramiento_Medios_de_Transporte' }, function (data) {
				$(".Detalle_Aseguramiento_Medios_de_TransporteBusinessRuleModal").html(data);
			});
		}

