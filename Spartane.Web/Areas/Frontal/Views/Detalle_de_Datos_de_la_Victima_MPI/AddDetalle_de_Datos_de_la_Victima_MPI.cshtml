		function SendDetalle_de_Datos_de_la_Victima_MPIData(callBack) {
			
					
		 var arr = []; 
		 $("form#CreateDetalle_de_Datos_de_la_Victima_MPI").find(':input').each(function( index ) {		 
			if ($(this).prop('disabled')==true|| $(this).prop('disabled')=="disabled")
			{		
				arr.push($(this).attr('id')); 
				$(this).prop('disabled', false);  	
			}
	    });
		
             var formData = new FormData($("form#CreateDetalle_de_Datos_de_la_Victima_MPI")[0]);
			$.each( arr, function( key, value ) {
			  $('#' +value).prop('disabled', true);
			});

            var isNew = $('#hdnIsNew').val();
           
            if (checkClientValidate('Detalle_de_Datos_de_la_Victima_MPICreateForm')) {
                $.ajax({
                    url: '@Url.Content("~/Frontal/Detalle_de_Datos_de_la_Victima_MPI/Post?IsNew=")' + isNew,
                    type: 'POST',
                    data: formData,
                    async: true,
                    success: function (result) {
                        if (result < 0) {
                            showNotification("@Resources.Resources.ErrorSave", "error");
                        } else {
                            SendDetalle_del_Abogado_Victima_MPI(callBack, result);

                            showNotification("@Resources.Resources.SaveSuccess", "success");
                            callBack(result);
                        }
                    },
                    error: function (result) {
                        showNotification("@Resources.Resources.ErrorContactingServer", "error");
                    },
                    cache: false,
                    contentType: false,
                    processData: false
                });
            }
            return false;
        };
		
