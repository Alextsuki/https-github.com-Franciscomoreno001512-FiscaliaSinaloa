                dataType: "json",
                async: false,
                success: function (result) {
                    Detalle_de_Galeria_de_TestigosDataDataTable = result.data;
                    showDetalle_de_Galeria_de_TestigosGrid(result.data);
                    $('#Detalle_de_Galeria_de_TestigosGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_de_Galeria_de_TestigosGrid .gridDatePicker').attr("readonly", "");
