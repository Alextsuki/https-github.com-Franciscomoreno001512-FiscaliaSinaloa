                keyboardNavigation: false,
                forceParse: false,
                calendarWeeks: true,
                autoclose: true,
                format: "@ConfigurationProperty.DateFormatDatePicker"
            });





            if(Detalle_de_Galeria_de_TestigosTable == null)
            {
                getDetalle_de_Galeria_de_TestigosData();
