        function getDetalle_de_Galeria_de_TestigosData() {
            $.ajax({
                url: "@Url.Content("~/Frontal/Detalle_de_Datos_del_Testigo_CC/GetDetalle_de_Galeria_de_Testigos")" + "?draw=1&start=1&length=2147483647" + ($("#ReferenceClave").val() != "" ? "&RelationId=" + $("#ReferenceClave").val() : ""),
