        function Adicciones_Probable_ResponsablerowCreationGrid(row, data, index) {
            var permission =  {            
                 Edit: '@permissionAdicciones_Probable_Responsable.Edit' == 'True',
                 Delete: '@permissionAdicciones_Probable_Responsable.Delete' == 'True'
            };
            setAdicciones_Probable_ResponsableValidation();
            var Adicciones_Probable_ResponsableRowElement = "Adicciones_Probable_Responsable_" + index.toString();
            $(row).addClass(Adicciones_Probable_ResponsableRowElement);
            var nameOfTable='Adicciones_Probable_Responsable_';
            var rowIndex = '_' + index;
            Adicciones_Probable_ResponsableinsertRowCurrentIndex = index;

            row.innerHTML = "";

            if (data.IsInsertRow) {

                if (row.childNodes.length != 0) {
                    for (var i = 0; i < row.childNodes.length; i++) {
                        row.childNodes[i].remove();
                    }
                }
                var controls = GetInsertAdicciones_Probable_ResponsableRowControls(index);
                var actionColInsert = $('<td>');

                var abc = "if(dynamicFieldValidation('" + Adicciones_Probable_ResponsableRowElement + "')){ Adicciones_Probable_ResponsableInsertRow(" + index + "); }";
                var insertRowHTML = '<a  data-toggle="tooltip" title="Confirmar Registro" onclick="' + abc + '">';
                $('<i class="fa fa-check">').appendTo($(insertRowHTML).appendTo(actionColInsert));
				$('<i class="fa fa-times">').appendTo($("<a data-toggle='tooltip' title='Cancelar Registro' onclick='Adicciones_Probable_ResponsableCancelRow(" + index + ")'>").appendTo(actionColInsert));
                actionColInsert.appendTo(row);

                for (i = 0; i < controls.length; i++) {
					var idHeader = $(controls[i]).data('field') + 'Header';
					if($(controls[i]).length > 1){
						idHeader = $($(controls[i])[1]).data('field') + 'Header';
					}					

					var classe = ($('#Adicciones_Probable_ResponsableGrid .' + idHeader).hasClass('dt-right') ? "dt-right" : "") + $('#Adicciones_Probable_ResponsableGrid .' + idHeader ).css('display') == 'none' ? " hide" : "";
				   
					if ($(controls[i]).next().length > 0) {
					    var div = $(controls[i]).next();
					    $('<td class="' + classe + '">').append($(controls[i])).append(div).appendTo(row);
					}else
					    $(controls[i]).appendTo($('<td class="' + classe + '">').appendTo(row));
					
                }
                $('#Adicciones_Probable_ResponsableGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                $('#Adicciones_Probable_ResponsableGrid .gridDatePicker').attr("readonly", "");
                return;
            }

            var actionCol = $('<td>');
			var edit = "<a title='@Resources.Resources.Edit' class='edit-MR' onclick='Adicciones_Probable_ResponsableEditRow(" + index + ", this)'>";
            if (!permission.Edit || isConsultMode)
                edit = "";
            var editPopUp = "<a title='@Resources.Resources.Edit' class='edit-popup-MR' data-toggle='modal' data-target='#AddAdicciones_Probable_Responsable-form' onclick='Adicciones_Probable_ResponsableEditRowPopup(" + index + ", this)'>";
            if (!permission.Edit || isConsultMode)
                editPopUp = "";
            var eliminar = "<a title='@Resources.Resources.Delete' class='eliminar-MR' onclick='Adicciones_Probable_ResponsableDeleteRow(" + index + ", this)'>";
            if (!permission.Delete || isConsultMode)
                eliminar = "";
            $('<i class="fa fa-pencil">').appendTo($(edit).appendTo(actionCol));
            //$('<i class="fa fa-edit">').appendTo($(editPopUp).appendTo(actionCol));
            $('<i class="fa fa-trash-o">').appendTo($(eliminar).appendTo(actionCol));
			if ($('.Action' + 'Header').css('display') != 'none')
				actionCol.appendTo(row);

            if ($('.Descripcion' + 'Header').css('display') != 'none')
                $('<td id="'+nameOfTable+'Descripcion'+rowIndex+'">').text(GetAdicciones_Probable_Responsable_AdiccionesName(data.Descripcion)).appendTo(row);
	    else 
		$('<td style="display:none;" id="'+nameOfTable+'Descripcion'+rowIndex+'">').text(GetAdicciones_Probable_Responsable_AdiccionesName(data.Descripcion)).appendTo(row);

        }

