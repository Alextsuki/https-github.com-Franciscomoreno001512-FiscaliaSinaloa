		function SendDetalle_de_ImputadoData(callBack) {
			
					
		 var arr = []; 
		 $("form#CreateDetalle_de_Imputado").find(':input').each(function( index ) {		 
			if ($(this).prop('disabled')==true|| $(this).prop('disabled')=="disabled")
			{		
				arr.push($(this).attr('id')); 
				$(this).prop('disabled', false);  	
			}
	    });
		
             var formData = new FormData($("form#CreateDetalle_de_Imputado")[0]);
			$.each( arr, function( key, value ) {
			  $('#' +value).prop('disabled', true);
			});

            var isNew = $('#hdnIsNew').val();
           
            if (checkClientValidate('Detalle_de_ImputadoCreateForm')) {
                $.ajax({
                    url: '@Url.Content("~/Frontal/Detalle_de_Imputado/Post?IsNew=")' + isNew,
                    type: 'POST',
                    data: formData,
                    async: true,
                    success: function (result) {
                        if (result < 0) {
                            showNotification("@Resources.Resources.ErrorSave", "error");
                        } else {
                            SendAdicciones_Probable_Responsable(callBack, result);
                            SendLugares_Frecuentes_Probable_Responsable(callBack, result);
                            SendDatos_Personales_Adicionales_Probable_Responsable(callBack, result);
                            SendOtras_Identificaciones_Probable_Responsable(callBack, result);
                            SendOtros_Domicilios_Probable_Responsable(callBack, result);
                            SendOtros_Nombres(callBack, result);

                            showNotification("@Resources.Resources.SaveSuccess", "success");
                            callBack(result);
                        }
                    },
                    error: function (result) {
                        showNotification("@Resources.Resources.ErrorContactingServer", "error");
                    },
                    cache: false,
                    contentType: false,
                    processData: false
                });
            }
            return false;
        };
		
