            var rowIndexTable = Otros_NombresTable.fnGetPosition($(row).parent().parent()[0]);
            var permission = {            
                 Delete: '@permissionOtros_Nombres.Delete' == 'True'
            };
            if (!permission.Delete || isConsultMode)
                return;            
            if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
              if (EjecutarValidacionesAlEliminarMROtros_Nombres('Otros_Nombres_', '_' + rowIndex)) {
                var prevData = Otros_NombresTable.fnGetData(rowIndexTable );
                removedOtros_NombresData.push(prevData);
                Otros_NombresTable.fnDeleteRow(rowIndexTable );
				showOtros_NombresGrid(Otros_NombresTable.fnGetData());
              }
            }
			initiateUIControls();
        }

        function Otros_NombresrowCreationGrid(row, data, index) {
            var permission =  {            
                 Edit: '@permissionOtros_Nombres.Edit' == 'True',
                 Delete: '@permissionOtros_Nombres.Delete' == 'True'
            };
            setOtros_NombresValidation();
            var Otros_NombresRowElement = "Otros_Nombres_" + index.toString();
            $(row).addClass(Otros_NombresRowElement);
            var nameOfTable='Otros_Nombres_';
            var rowIndex = '_' + index;
            Otros_NombresinsertRowCurrentIndex = index;

            row.innerHTML = "";

            if (data.IsInsertRow) {

                if (row.childNodes.length != 0) {
                    for (var i = 0; i < row.childNodes.length; i++) {
                        row.childNodes[i].remove();
                    }
                }
                var controls = GetInsertOtros_NombresRowControls(index);
                var actionColInsert = $('<td>');

                var abc = "if(dynamicFieldValidation('" + Otros_NombresRowElement + "')){ Otros_NombresInsertRow(" + index + "); }";
                var insertRowHTML = '<a  data-toggle="tooltip" title="Confirmar Registro" onclick="' + abc + '">';
                $('<i class="fa fa-check">').appendTo($(insertRowHTML).appendTo(actionColInsert));
				$('<i class="fa fa-times">').appendTo($("<a data-toggle='tooltip' title='Cancelar Registro' onclick='Otros_NombresCancelRow(" + index + ")'>").appendTo(actionColInsert));
                actionColInsert.appendTo(row);

                for (i = 0; i < controls.length; i++) {
					var idHeader = $(controls[i]).data('field') + 'Header';
					if($(controls[i]).length > 1){
						idHeader = $($(controls[i])[1]).data('field') + 'Header';
					}					

					var classe = ($('#Otros_NombresGrid .' + idHeader).hasClass('dt-right') ? "dt-right" : "") + $('#Otros_NombresGrid .' + idHeader ).css('display') == 'none' ? " hide" : "";
				   
					if ($(controls[i]).next().length > 0) {
					    var div = $(controls[i]).next();
					    $('<td class="' + classe + '">').append($(controls[i])).append(div).appendTo(row);
					}else
					    $(controls[i]).appendTo($('<td class="' + classe + '">').appendTo(row));
					
                }
                $('#Otros_NombresGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                $('#Otros_NombresGrid .gridDatePicker').attr("readonly", "");
                return;
            }

            var actionCol = $('<td>');
			var edit = "<a title='@Resources.Resources.Edit' class='edit-MR' onclick='Otros_NombresEditRow(" + index + ", this)'>";
            if (!permission.Edit || isConsultMode)
                edit = "";
            var editPopUp = "<a title='@Resources.Resources.Edit' class='edit-popup-MR' data-toggle='modal' data-target='#AddOtros_Nombres-form' onclick='Otros_NombresEditRowPopup(" + index + ", this)'>";
            if (!permission.Edit || isConsultMode)
                editPopUp = "";
            var eliminar = "<a title='@Resources.Resources.Delete' class='eliminar-MR' onclick='Otros_NombresDeleteRow(" + index + ", this)'>";
            if (!permission.Delete || isConsultMode)
                eliminar = "";
            $('<i class="fa fa-pencil">').appendTo($(edit).appendTo(actionCol));
            //$('<i class="fa fa-edit">').appendTo($(editPopUp).appendTo(actionCol));
            $('<i class="fa fa-trash-o">').appendTo($(eliminar).appendTo(actionCol));
			if ($('.Action' + 'Header').css('display') != 'none')
				actionCol.appendTo(row);

            if ($('.Descripcion' + 'Header').css('display') != 'none')
                $('<td id="'+nameOfTable+'Descripcion'+rowIndex+'">').text(data.Descripcion).appendTo(row);
            else 
                $('<td style="display:none;" id="'+nameOfTable+'Descripcion'+rowIndex+'">').text(data.Descripcion).appendTo(row);

        }

