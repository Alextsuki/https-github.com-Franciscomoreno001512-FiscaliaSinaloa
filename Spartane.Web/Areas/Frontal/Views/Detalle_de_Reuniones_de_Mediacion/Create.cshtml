		function Fill_Datos_Generales()
        {
            $.ajax({
                url: "@Url.Content("~/Frontal/Detalle_de_Reuniones_de_Mediacion/Get_Datos_Generales")?Id=" + $("#ReferenceClave").val(),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                                        $('#Clave').val(result.data.Clave);
                    var control = $('#Numero_de_Expediente');
                    control.select2('open');
                    $('.select2-search__field').val(result.data.Numero_de_Expediente).trigger('keyup');
                    control.select2('close');
                    setTimeout(function () {
	                    var data = eval('AutoComplete' + control.selector.replace('#', '') + 'Data');
	                    control.select2({ data: data });
	                    control.val(result.data.Numero_de_Expediente).trigger('change');
                    }, 3000);
                    $('#Fecha_de_Registro').val(result.data.Fecha_de_Registro);
                    $('#Hora_de_Registro').val(result.data.Hora_de_Registro);
                    var control = $('#Usuario_que_Registra');
                    control.select2('open');
                    $('.select2-search__field').val(result.data.Usuario_que_Registra).trigger('keyup');
                    control.select2('close');
                    setTimeout(function () {
	                    var data = eval('AutoComplete' + control.selector.replace('#', '') + 'Data');
	                    control.select2({ data: data });
	                    control.val(result.data.Usuario_que_Registra).trigger('change');
                    }, 3000);
                    $('#Lugar').val(result.data.Lugar);
                    $('#Fecha').val(result.data.Fecha);
                    $('#Hora').val(result.data.Hora);
                    var control = $('#Especialista');
                    control.select2('open');
                    $('.select2-search__field').val(result.data.Especialista).trigger('keyup');
                    control.select2('close');
                    setTimeout(function () {
	                    var data = eval('AutoComplete' + control.selector.replace('#', '') + 'Data');
	                    control.select2({ data: data });
	                    control.val(result.data.Especialista).trigger('change');
                    }, 3000);
                    $('#Tipo_de_Sesion').val(result.data.Tipo_de_Sesion);
                    Detalle_de_Solicitante_en_ReunionesDataDataTable = result.Solicitanter;
                    showDetalle_de_Solicitante_en_ReunionesGrid(result.Solicitanter);
                    $('#Detalle_de_Solicitante_en_ReunionesGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_de_Solicitante_en_ReunionesGrid .gridDatePicker').attr("readonly", "");
                    Detalle_de_Requerido_en_ReunionesDataDataTable = result.Requeridor;
                    showDetalle_de_Requerido_en_ReunionesGrid(result.Requeridor);
                    $('#Detalle_de_Requerido_en_ReunionesGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_de_Requerido_en_ReunionesGrid .gridDatePicker').attr("readonly", "");
                    $('#Cumplimiento').val(result.data.Cumplimiento);

                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
		
		function Save_Datos_Generales(newId) {
			var data = new FormData();
			data.append('Clave', newId);
			                data.append('Numero_de_Expediente', $('#Numero_de_Expediente').val());
                data.append('Fecha_de_Registro', $('#Fecha_de_Registro').val());
                data.append('Hora_de_Registro', $('#Hora_de_Registro').val());
                data.append('Usuario_que_Registra', $('#Usuario_que_Registra').val());
                data.append('Lugar', $('#Lugar').val());
                data.append('Fecha', $('#Fecha').val());
                data.append('Hora', $('#Hora').val());
                data.append('Especialista', $('#Especialista').val());
                data.append('Tipo_de_Sesion', $('#Tipo_de_Sesion').val());
                data.append('Cumplimiento', $('#Cumplimiento').val());

			$.ajax({
				url: '@Url.Content("~/Frontal/Detalle_de_Reuniones_de_Mediacion/Post_Datos_Generales")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
				contentType: false,
				processData: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "Error al guardar Datos_Generales");
					} else {
						newId = result;
                    					    SendDetalle_de_Solicitante_en_Reuniones(null, newId);
					    SendDetalle_de_Requerido_en_Reuniones(null, newId);

					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "Error al guardar Datos_Generales");
				},
				cache: false
			});
		}


