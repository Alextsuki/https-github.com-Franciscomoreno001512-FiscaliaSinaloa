                    $('.select2-search__field').val(result.data.Usuario_que_Registra).trigger('keyup');
                    control.select2('close');
                    setTimeout(function () {
	                    var data = eval('AutoComplete' + control.selector.replace('#', '') + 'Data');
	                    control.select2({ data: data });
	                    control.val(result.data.Usuario_que_Registra).trigger('change');
                    }, 3000);
                    $('#Numero_de_Invitacion').val(result.data.Numero_de_Invitacion);
                    $('#Numero_de_Oficio').val(result.data.Numero_de_Oficio);
                    $('#Formato_de_Invitacion').val(result.data.Formato_de_Invitacion);
                    $('#Tipo_de_Participacion').val(result.data.Tipo_de_Participacion);
                    $('#Fecha_de_la_cita').val(result.data.Fecha_de_la_cita);
                    $('#Hora_de_la_Cita').val(result.data.Hora_de_la_Cita);
                    $('#Domicilio').val(result.data.Domicilio);
                    Detalle_de_Solicitante_en_InvitacionesDataDataTable = result.nombre_del_interviniente_solicitante;
                    showDetalle_de_Solicitante_en_InvitacionesGrid(result.nombre_del_interviniente_solicitante);
                    $('#Detalle_de_Solicitante_en_InvitacionesGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
