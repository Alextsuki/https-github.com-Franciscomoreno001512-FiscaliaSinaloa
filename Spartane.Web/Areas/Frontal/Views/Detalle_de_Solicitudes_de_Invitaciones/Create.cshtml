                data.append('Hora_de_la_Cita', $('#Hora_de_la_Cita').val());
                data.append('Domicilio', $('#Domicilio').val());

			$.ajax({
				url: '@Url.Content("~/Frontal/Detalle_de_Solicitudes_de_Invitaciones/Post_Datos_Generales")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
