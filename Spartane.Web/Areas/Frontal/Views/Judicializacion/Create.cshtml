                var controls = GetInsertDetalle_Vinculacion_JudicializacionRowControls(index);
                var actionColInsert = $('<td>');

                var abc = "if(dynamicFieldValidation('" + Detalle_Vinculacion_JudicializacionRowElement + "')){ Detalle_Vinculacion_JudicializacionInsertRow(" + index + "); }";
                var insertRowHTML = '<a  data-toggle="tooltip" title="Confirmar Registro" onclick="' + abc + '">';
                $('<i class="fa fa-check">').appendTo($(insertRowHTML).appendTo(actionColInsert));
