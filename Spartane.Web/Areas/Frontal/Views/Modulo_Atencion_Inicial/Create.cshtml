        function Detalle_de_Canalizar_EstatusrowCreationGrid(row, data, index) {
            var permission =  {            
                 Edit: '@permissionDetalle_de_Canalizar_Estatus.Edit' == 'True',
                 Delete: '@permissionDetalle_de_Canalizar_Estatus.Delete' == 'True'
            };
            setDetalle_de_Canalizar_EstatusValidation();
            var Detalle_de_Canalizar_EstatusRowElement = "Detalle_de_Canalizar_Estatus_" + index.toString();
            $(row).addClass(Detalle_de_Canalizar_EstatusRowElement);
            var nameOfTable='Detalle_de_Canalizar_Estatus_';
            var rowIndex = '_' + index;
            Detalle_de_Canalizar_EstatusinsertRowCurrentIndex = index;

            row.innerHTML = "";

            if (data.IsInsertRow) {

                if (row.childNodes.length != 0) {
                    for (var i = 0; i < row.childNodes.length; i++) {
                        row.childNodes[i].remove();
                    }
                }
                var controls = GetInsertDetalle_de_Canalizar_EstatusRowControls(index);
                var actionColInsert = $('<td>');

                var abc = "if(dynamicFieldValidation('" + Detalle_de_Canalizar_EstatusRowElement + "')){ Detalle_de_Canalizar_EstatusInsertRow(" + index + "); }";
                var insertRowHTML = '<a  data-toggle="tooltip" title="Confirmar Registro" onclick="' + abc + '">';
                $('<i class="fa fa-check">').appendTo($(insertRowHTML).appendTo(actionColInsert));
				$('<i class="fa fa-times">').appendTo($("<a data-toggle='tooltip' title='Cancelar Registro' onclick='Detalle_de_Canalizar_EstatusCancelRow(" + index + ")'>").appendTo(actionColInsert));
                actionColInsert.appendTo(row);

                for (i = 0; i < controls.length; i++) {
					var idHeader = $(controls[i]).data('field') + 'Header';
					if($(controls[i]).length > 1){
						idHeader = $($(controls[i])[1]).data('field') + 'Header';
					}					

					var classe = ($('#Detalle_de_Canalizar_EstatusGrid .' + idHeader).hasClass('dt-right') ? "dt-right" : "") + $('#Detalle_de_Canalizar_EstatusGrid .' + idHeader ).css('display') == 'none' ? " hide" : "";
				   
					if ($(controls[i]).next().length > 0) {
					    var div = $(controls[i]).next();
					    $('<td class="' + classe + '">').append($(controls[i])).append(div).appendTo(row);
					}else
					    $(controls[i]).appendTo($('<td class="' + classe + '">').appendTo(row));
					
                }
                $('#Detalle_de_Canalizar_EstatusGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                $('#Detalle_de_Canalizar_EstatusGrid .gridDatePicker').attr("readonly", "");
                return;
            }

            var actionCol = $('<td>');
			var edit = "<a title='@Resources.Resources.Edit' class='edit-MR' onclick='Detalle_de_Canalizar_EstatusEditRow(" + index + ", this)'>";
            if (!permission.Edit || isConsultMode)
                edit = "";
            var editPopUp = "<a title='@Resources.Resources.Edit' class='edit-popup-MR' data-toggle='modal' data-target='#AddDetalle_de_Canalizar_Estatus-form' onclick='Detalle_de_Canalizar_EstatusEditRowPopup(" + index + ", this)'>";
