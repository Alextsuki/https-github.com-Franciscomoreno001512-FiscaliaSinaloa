        function OrientadorGetBusinessRule(FieldId, FieldName, Attribute) {
			if (FieldId != null && FieldName != null)
			{
				$('#hdnOrientadorBusinessRuleFieldId').val(FieldId);
				$('#hdnOrientadorBusinessRuleFieldName').val(FieldName);
				$('#hdnOrientadorBusinessRuleAttribute').val(Attribute);
			}
            $.get('@Url.Action("BusinessRule", "BussinessRule")', { FieldId: $('#hdnOrientadorBusinessRuleFieldId').val(), FieldName: $('#hdnOrientadorBusinessRuleFieldName').val(), ObjectId: '44998', Attribute: $('#hdnOrientadorBusinessRuleAttribute').val(), Screen: 'Orientador' }, function (data) {
                $(".OrientadorBusinessRuleModal").html(data);
            });
        }
        function OrientadorGetAddBusinessRule(RuleId, FieldId, FieldName, Attribute) {
			if (FieldId != null && FieldName != null) {
				$('#hdnOrientadorBusinessRuleFieldId').val(FieldId);
				$('#hdnOrientadorBusinessRuleFieldName').val(FieldName);
				$('#hdnOrientadorBusinessRuleAttribute').val(Attribute);
			}
            $.get('@Url.Action("AddBusinessRule", "BussinessRule")', { BusinessRuleId: RuleId, FieldId: $('#hdnOrientadorBusinessRuleFieldId').val(), FieldName: $('#hdnOrientadorBusinessRuleFieldName').val(), ObjectId: '44998', Attribute: $('#hdnOrientadorBusinessRuleAttribute').val(), Screen: 'Orientador' }, function (data) {
                $(".OrientadorAddBusinessRuleModal").html(data);
                $(".datatable").DataTable();
            });
        }
