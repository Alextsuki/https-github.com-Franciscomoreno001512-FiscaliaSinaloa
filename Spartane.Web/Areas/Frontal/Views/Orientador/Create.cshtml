<script type="text/javascript">


   function GetCatalogPopup(catalog, nameAttribute, isMR, nameMR) {
        if (!isMR)
            var url = "@Url.Action("Create", "Catalog", new { Module_Id = "1", isPartial = true, nameAttribute = "AttributoNombre" })";
        else
            var url = "@Url.Action("Create", "Catalog", new { Module_Id = "1", isPartial = true, isMR = true, nameMR = "Multirenglon",nameAttribute= "AttributoNombre" })";

        url = url.replace("Catalog", catalog).replace("Multirenglon", nameMR).replace("AttributoNombre", nameAttribute);

        window.open(htmlDecode(url), '_blank', 'left=200,top=100,width=850,height=550,toolbar=0,resizable=0');
    }

    function RefreshCatalog(catalog, control) {

	 if ($('#' + control).hasClass('AutoComplete'))   {
		   if ($('#' + control).attr("data-isfilter") == "true") {
				eval(GetReglaFilter(control, $('#ObjectId').val()));
				return;
			}
		}
			
			var url = '@Url.Content("~/Frontal/Orientador/GetCatalogAll")';
			url = url.replace(/Catalog/g, catalog);

			$.ajax({
				url: url,
				async: true,
				success:function(result)
				{
					addItemsToSelect($('#' + control), result);
				},
				error: function (result)
				{
					showNotification("@Resources.Resources.ErrorContactingServer", "error");
				}
			});
		
    }


        function SendOrientadorData(callBack) {
			
					
		 var arr = []; 
		 $("form#CreateOrientador").find(':input').each(function( index ) {		 
			if ($(this).prop('disabled')==true|| $(this).prop('disabled')=="disabled")
			{		
				arr.push($(this).attr('id')); 
				$(this).prop('disabled', false);  	
			}
	    });
		
             var formData = new FormData($("form#CreateOrientador")[0]);
			$.each( arr, function( key, value ) {
			  $('#' +value).prop('disabled', true);
			});
			var id = $("#ReferenceClave").val();
            var isNew = $('#hdnIsNew').val();
           
			if (Tab_Datos_de_Servicios_de_Apoyo && !checkClientValidate('OrientadorCreateForm', 'Datos_de_Servicios_de_Apoyo')) {
				return false;
			}						

			if (Tab_Datos_del_Imputado && !checkClientValidate('OrientadorCreateForm', 'Datos_del_Imputado')) {
				return false;
			}						

			if (Tab_Datos_de_los_Hechos && !checkClientValidate('OrientadorCreateForm', 'Datos_de_los_Hechos')) {
				return false;
			}						

			if (Tab_Datos_del_Delito && !checkClientValidate('OrientadorCreateForm', 'Datos_del_Delito')) {
				return false;
			}						

			if (Tab_Datos_de_Robo_de_Vehiculo && !checkClientValidate('OrientadorCreateForm', 'Datos_de_Robo_de_Vehiculo')) {
				return false;
			}						

			if (Tab_Datos_de_Levantamiento_de_Cadaver && !checkClientValidate('OrientadorCreateForm', 'Datos_de_Levantamiento_de_Cadaver')) {
				return false;
			}						

			if (Tab_Imputado && !checkClientValidate('OrientadorCreateForm', 'Imputado')) {
				return false;
			}						


			
			if (isNew == "True")
            {
                id = CreateID();
            }
			
			
			if (Tab_Datos_de_Servicios_de_Apoyo)
				Save_Datos_de_Servicios_de_Apoyo(id);

			if (Tab_Datos_del_Imputado)
				Save_Datos_del_Imputado(id);

			if (Tab_Datos_de_los_Hechos)
				Save_Datos_de_los_Hechos(id);

			if (Tab_Datos_del_Delito)
				Save_Datos_del_Delito(id);

			if (Tab_Datos_de_Robo_de_Vehiculo)
				Save_Datos_de_Robo_de_Vehiculo(id);

			if (Tab_Datos_de_Levantamiento_de_Cadaver)
				Save_Datos_de_Levantamiento_de_Cadaver(id);

			if (Tab_Imputado)
				Save_Imputado(id);


			showNotification("@Resources.Resources.SaveSuccess", "success");
			callBack(id);
            return true;
        };
		function CreateID()
		{
			var newId = 0;
			$.ajax({
				url: '@Url.Content("~/Frontal/Orientador/CreateID")',
				type: 'GET',
				async: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "error");
					} else {
						newId = result;
					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "error");
				},
				cache: false,
				contentType: false,
				processData: false
			});
			return newId;
		}
		
