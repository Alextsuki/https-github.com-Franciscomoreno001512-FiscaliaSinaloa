                data.append('Acuerdos', $('#Acuerdos').val());

			$.ajax({
				url: '@Url.Content("~/Frontal/Plan_de_Investigacion/Post_Datos_Generales")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
				contentType: false,
				processData: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "Error al guardar Datos_Generales");
					} else {
						newId = result;
                    
					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "Error al guardar Datos_Generales");
				},
				cache: false
			});
		}

		function Fill_Actos_de_Investigacion()
        {
            $.ajax({
                url: "@Url.Content("~/Frontal/Plan_de_Investigacion/Get_Actos_de_Investigacion")?Id=" + $("#ReferenceClave").val(),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                                        $('#Clave').val(result.data.Clave);
                    Detalle_Plan_Actos_de_InvestigacionDataDataTable = result.Detalle;
                    showDetalle_Plan_Actos_de_InvestigacionGrid(result.Detalle);
                    $('#Detalle_Plan_Actos_de_InvestigacionGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_Plan_Actos_de_InvestigacionGrid .gridDatePicker').attr("readonly", "");

                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
		
		function Save_Actos_de_Investigacion(newId) {
			var data = new FormData();
			data.append('Clave', newId);
			
			$.ajax({
				url: '@Url.Content("~/Frontal/Plan_de_Investigacion/Post_Actos_de_Investigacion")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
				contentType: false,
				processData: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "Error al guardar Actos_de_Investigacion");
					} else {
						newId = result;
                    					    SendDetalle_Plan_Actos_de_Investigacion(null, newId);

					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "Error al guardar Actos_de_Investigacion");
				},
				cache: false
			});
		}


    </script>
