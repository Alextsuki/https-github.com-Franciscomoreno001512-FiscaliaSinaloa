        function Detalle_Solicitud_Historial_de_AsignacionesGetUpdateRowControls(data, nameOfTable, rowIndex) {

            var columnData = [];
            var counter = 0;
            var inputData = "<input type='text' class='fullWidth form-control' />";

            columnData[counter++] = $($($.parseHTML(GetGridDatePicker())).val(data.Fecha_cambio)).addClass('Fecha_cambio').attr('id', nameOfTable + 'Fecha_cambio' + rowIndex).attr('data-field', 'Fecha_cambio');
            columnData[counter++] = $($($.parseHTML(GetGridTimePicker())).val(data.Hora_cambio)).addClass('Hora_cambio').attr('id', nameOfTable + 'Hora_cambio' + rowIndex).attr('data-field', 'Hora_cambio');
            columnData[counter++] = $($($.parseHTML(GetGridAutoComplete(data.UsuarioName != undefined ? data.UsuarioName : null, 'AutoComplete' + nameOfTable + 'Usuario', data.Usuario)))).addClass('Usuario').attr('id', nameOfTable + 'Usuario' + rowIndex).attr('data-field', 'Usuario').after($.parseHTML(addNew('Detalle_Solicitud_Historial_de_Asignaciones', 'Spartan_User','Usuario', 266705)));
            columnData[counter++] = $($($.parseHTML(GetGridAutoComplete(data.Facilitador_AsignadoName != undefined ? data.Facilitador_AsignadoName : null, 'AutoComplete' + nameOfTable + 'Facilitador_Asignado', data.Facilitador_Asignado)))).addClass('Facilitador_Asignado').attr('id', nameOfTable + 'Facilitador_Asignado' + rowIndex).attr('data-field', 'Facilitador_Asignado').after($.parseHTML(addNew('Detalle_Solicitud_Historial_de_Asignaciones', 'Spartan_User','Facilitador_Asignado', 266706)));
            columnData[counter++] = $($($.parseHTML(inputData)).val(data.Motivo_cambio)).addClass('Motivo_cambio').attr('id', nameOfTable + 'Motivo_cambio' + rowIndex).attr('data-field', 'Motivo_cambio');

            return columnData;
        }



        function GetFileInfo(fileData, fileUploadData) {

            var $div = $("<div>");
            if (fileUploadData != null && fileUploadData.files.length > 0) {

                $div.append("<label>" + fileUploadData.files[0].name + "</label>");
                //<i class="fa fa-times-circle" id='removeFileAttachment'>
                $div.append($("<i class='fa fa-times-circle' id='removeFileAttachment'>").click(function () {
                    if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
                        var parent = $(this).parent().parent();
                        parent.empty();
                        parent.append("<input type='file'>");
                    }
                }));
                return $div;

            } else if (fileUploadData != null) {
                return $.parseHTML(GetFileUploader());
            } else {
                $div.append("<label>" + fileData.FileName + "</label>");
                $div.append($("<i class='fa fa-times-circle' id='removeFileAttachment'>").click(function () {
                    if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
                        var parent = $(this).parent().parent();
                        parent.empty();
                        parent.append("<input type='file'>");
                    }
                }));
                return $div;
            }
        }

