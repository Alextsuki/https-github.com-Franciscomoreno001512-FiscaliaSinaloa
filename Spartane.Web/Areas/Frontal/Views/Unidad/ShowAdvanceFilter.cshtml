        advanceSearch.Clave_de_MunicipioSearchType = 0;
        if ($('#ddlAdvanceClave_de_Municipio').val().length > 0 || $("#txtAdvanceClave_de_Municipio").val() == "") {


            if (isUnidadTextSearch) {
                advanceSearch.Clave_de_MunicipioName = $("#txtAdvanceClave_de_Municipio").val();
                advanceSearch.Clave_de_MunicipioNameSearchType = $("#ddlAdvanceClave_de_Municipio").val();
                advanceSearch.Clave_de_MunicipioSearchType = 1;
            }
            else {

                advanceSearch.Clave_de_MunicipioToSearch = $("#ddlAdvanceClave_de_Municipio").val();
                advanceSearch.Clave_de_MunicipioSearchType = 2;
            }
        }
        advanceSearch.Clave_de_RegionSearchType = 0;
