<script>
    function InitiateAdvanceSearch() {
        var advanceSearch = {}, hasParameter = false;
        if ($("#txtAdvanceFromClave").val() != "" || $("#txtAdvanceToClave").val() != "") {
            hasParameter = true;
            advanceSearch.UnidadClaveFrom = $("#txtAdvanceFromClave").val();
            advanceSearch.UnidadClaveTo = $("#txtAdvanceToClave").val();
        }
        if ($("#txtAdvanceFromClave_Unica_Municipio").val() != "" || $("#txtAdvanceToClave_Unica_Municipio").val() != "") {
            hasParameter = true;
            advanceSearch.UnidadClave_Unica_MunicipioFrom = $("#txtAdvanceFromClave_Unica_Municipio").val();
            advanceSearch.UnidadClave_Unica_MunicipioTo = $("#txtAdvanceToClave_Unica_Municipio").val();
